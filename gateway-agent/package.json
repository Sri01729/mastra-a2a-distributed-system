{
  "name": "gateway-agent",
  "version": "1.0.0",
  "description": "Gateway Agent for Mastra A2A Communication",
  "main": "dist/gateway-agent.js",
  "scripts": {
    "build": "tsc",
    "build:proxy": "tsc src/proxy.ts --outDir dist",
    "start": "node dist/gateway-agent.js",
    "start:proxy": "node dist/proxy.js",
    "dev": "ts-node src/gateway-agent.ts",
    "dev:proxy": "ts-node src/proxy.ts"
  },
  "dependencies": {
    "@mastra/client-js": "^0.10.23",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
