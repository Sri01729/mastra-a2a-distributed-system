# Gateway Agent Environment Configuration
# Copy this file to .env and update with your actual Mastra Cloud URLs

# Research Agent URL (from Mastra Cloud deployment)
RESEARCH_AGENT_URL=https://your-research-agent.mastra.ai

# Writing Agent URL (from Mastra Cloud deployment)
WRITING_AGENT_URL=https://your-writing-agent.mastra.ai

# Analysis Agent URL (from Mastra Cloud deployment)
ANALYSIS_AGENT_URL=https://your-analysis-agent.mastra.ai

# Gateway Agent Configuration
GATEWAY_PORT=3001
NODE_ENV=production

# Optional: Add any other environment variables your gateway needs
# OPENAI_API_KEY=your-openai-api-key
# OTHER_API_KEY=your-other-api-key
